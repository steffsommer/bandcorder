  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug mobile app",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug"
    },
    {
      "name": "Run mobile app",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Debug Server/Desktop App",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/server/main.py",
      "console": "integratedTerminal"
    }
  ]
}